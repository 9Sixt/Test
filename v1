Loop(function()
	if C()['Berry Find'] then
		pcall(function()
			for _, instance in pairs(CollectionService:GetTagged("BerryBush")) do
				for i, v in pairs(instance:GetAttributes()) do
					if i:sub(1, 12) == "_BerryCFrame" then
						tp(instance.Parent:GetPivot())
						break
					end
				end
				for _, child in pairs(instance:GetChildren()) do
					for i, v in pairs(child:GetChildren()) do
						if string.match(v.Name, "Sphere") then
							tp(v.CFrame)
							if (v.Position - Character:GetPivot().Position).Magnitude < 10 then
								VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, HumanoidRootPart)
								VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, HumanoidRootPart)
							end
						end
					end
				end
			end
		end)
	end
end)

Loop(function()
	if C()['Chest'] then
		pcall(function()
			local Chests = CollectionService:GetTagged("_ChestTagged")
			local Distance, Nearest = math.huge, nil
			for i = 1, #Chests do
				local Chest = Chests[i]
				local Magnitude = (Chest:GetPivot().Position - HumanoidRootPart.Position).Magnitude
				if Chest:GetAttribute("IsDisabled") ~= true and Chest:GetAttributes() ~= nil and Magnitude < Distance then
					Distance, Nearest = Magnitude, Chest
				end
			end
			if Nearest then
				tp(Nearest:GetPivot())
			end
		end)
	end
end)

