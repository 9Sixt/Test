Window = Fetching:Window({
	Logo = 128185233852701,
	Size = UDim2.new(0, 500,0, 375),
	MainColor = Color3.fromRGB(0, 170, 255),
	DropColor = Color3.fromRGB(105, 94, 255),
	Keybind = Enum.KeyCode.B
})

_General = Window:CreateTab({Title = translate("General", "ทั่วไป"),Icon = Icons.thunder});
_Auto = Window:CreateTab({Title = translate("Automatic", "อัตโนมัติ"),Icon = Icons.special});
_Items = Window:CreateTab({Title = translate("Items", "ไอเท็ม"),Icon = Icons.star});
_Shop = Window:CreateTab({Title = translate("Shop", "ร้านค้า"),Icon = Icons.shop});
_Visual = Window:CreateTab({Title = translate("Visual", "การมองเห็น"),Icon = Icons.eye});
_Teleport = Window:CreateTab({Title = translate("Teleport", "เทเลพอร์ต"),Icon = Icons.location});
_Misc = Window:CreateTab({Title = translate("Others", "เพิ่มเติม"),Icon = Icons.jigsaw});

function Section(p, t, s)
	return p:CreateSection({
		Title = t,
		Side = s
	});
end

function Toggle(s, t, v, c)
	return s:CreateToggle({
		Title = t,
		Value = C()[v],
		CallBack = function(value)
			if c then
				c(value)
			end
			C()[v] = value
			Save(v, value)
		end
	})
end

function Button(s, t, c)
	return s:CreateButton({
		Title = t,
		Callback = function()
			c()
		end
	})
end

function ButtonImage(s, t, d, i, c)
	return s:ButtonImage({
		Title = t,
		Desc = d,
		Icon = "http://www.roblox.com/asset/?id=" .. tostring(i),
		Callback = function()
			c()
		end
	})
end


function ToggleImage(s, t, i, v, c)
	return s:ImageToggle({
		Title = t,
		Icon = 'http://www.roblox.com/asset/?id=' .. i,
		Value = C()[v],
		CallBack = function(value)
			if c then
				c(value)
			end
			C()[v] = value
			Save(v, value)
		end
	})
end

