							ComF("LoadFruit", GetfruitstoreW3())
						end
					end
				else
					if ComF("BartiloQuestProgress","Bartilo") == 0 then
						if PlayerGui.Main.Quest then
							Kill({Monster = 'Swan Pirate',WaitMonster = true,Bring = true,Stop = function()
								return not C()['Third Sea']
							end})
						else
							if (CFrame.new(-461, 72, 300).Position - HumanoidRootPart.Position).Magnitude <= 3 then
								ComF("StartQuest", "BartiloQuest", 1)
							else
								tp(CFrame.new(-461, 72, 300))
							end
						end
					elseif ComF("BartiloQuestProgress","Bartilo") == 1 then
						Kill({Monster = 'Jeremy',WaitMonster = false,Bring = false,Stop = function()
							return not C()['Third Sea']
						end})
					elseif ComF("BartiloQuestProgress","Bartilo") == 2 then
						if (Vector3.new(-1830, 10, 1680) - Humanoid.Position).Magnitude > 50 then
							tp(CFrame.new(-1830, 10, 1680))
						else
							HumanoidRootPart.CFrame = workspace.Map.Dressrosa.BartiloPlates.Plate1.CFrame
							wait(0.7)
							HumanoidRootPart.CFrame = workspace.Map.Dressrosa.BartiloPlates.Plate2.CFrame
							wait(0.7)
							HumanoidRootPart.CFrame = workspace.Map.Dressrosa.BartiloPlates.Plate3.CFrame
							wait(0.7)
							HumanoidRootPart.CFrame = workspace.Map.Dressrosa.BartiloPlates.Plate4.CFrame
							wait(0.7)
							HumanoidRootPart.CFrame = workspace.Map.Dressrosa.BartiloPlates.Plate5.CFrame
							wait(0.7)
							HumanoidRootPart.CFrame = workspace.Map.Dressrosa.BartiloPlates.Plate6.CFrame
							wait(0.7)
							HumanoidRootPart.CFrame = workspace.Map.Dressrosa.BartiloPlates.Plate7.CFrame
							wait(0.7)
							HumanoidRootPart.CFrame = workspace.Map.Dressrosa.BartiloPlates.Plate8.CFrame
						end
					end
				end
			end
		end)
	end
end)

