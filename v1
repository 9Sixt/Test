local FruitPrice, FruitStore = {}, {}
local whitelistedfruit = {}
local TableDevilFruit = {}
local m1 = {}
for i,v in next,ComF("GetFruits") do
	if v.Price <= 1000000 then
		table.insert(whitelistedfruit,string.split(v.Name,"-")[1].." Fruit")
		m1[v.Name] = v.Price
	end
	table.insert(TableDevilFruit,v.Name)
end

function Getfruitstore(x)
	local max = math.huge
	local name 
	for i,v in next,ComF("getInventory") do
		if v.Type == "Blox Fruit" then
			if not x then
				for i2, v2 in pairs(m1) do
					if v.Name == i2 then
						if tonumber(v2) < tonumber(max) then
							max = v2
							name = i2
						end
					end
				end
			else
				if not m1[v.Name] then
					return v.Name
				end
			end
		end
	end
	return name
end

function Checkdevilfruit()
	local name
	for i,v in pairs(Backpack:GetChildren()) do
		if string.find(v.Name,"Fruit")  then
			name = v.Name
		end
	end
	for i,v in pairs(LocalPlayer.Character:GetChildren()) do
		if string.find(v.Name,"Fruit")  then
			name = v.Name
		end
	end
	return name
end

WeAreLoop(function()
	if C()['Auto Unstored Fruit'] then
		if not Checkdevilfruit() and Getfruitstore() then
			ComF("LoadFruit", Getfruitstore())
		end
	end
	wait(5)
end)

WeAreLoop(function()
	local MobT = {
		"Reborn Skeleton",
		"Living Zombie",
		"Demonic Soul",
		"Posessed Mummy"
	}
