function Section(p, t, s)
	return p:CreateSection({
		Title = t,
		Side = s
	});
end

function Toggle(s, t, v, c)
	return s:CreateToggle({
		Title = t,
		Value = C()[v],
		CallBack = function(value)
			if c then
				c(value)
			end
			C()[v] = value
			Save(v, value)
		end
	})
end

function Button(s, t, c)
	return s:CreateButton({
		Title = t,
		Callback = function()
			c()
		end
	})
end

function ButtonImage(s, t, d, i, c)
	return s:ButtonImage({
		Title = t,
		Desc = d,
		Icon = "http://www.roblox.com/asset/?id=" .. tostring(i),
		Callback = function()
			c()
		end
	})
end


function ToggleImage(s, t, i, v, c)
	return s:ImageToggle({
		Title = t,
		Icon = 'http://www.roblox.com/asset/?id=' .. i,
		Value = C()[v],
		CallBack = function(value)
			if c then
				c(value)
			end
			C()[v] = value
			Save(v, value)
		end
	})
end

function Slider(s, t, min, max, v, r)
	return s:CreateSlider({
		Title = t,
		Min = min,
		Max = max,
		Value = C()[v],
		Rounding = 1,
		CallBack = function(value)
			C()[v] = value
			Save(v, value)
		end
	})
end

function Selection(s, t, l, v)
	return s:CreateSelect({
		Title = t,
		List = l,
		Value = C()[v],
		Callback = function(value)
			C()[v] = value
			Save(v, value)
		end
	})
end

