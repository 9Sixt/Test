	if C()['Second Sea'] then
		pcall(function()
			if Env_.w1 and LocalPlayer.Data.Level.Value >= 700 then
				if ComF("DressrosaQuestProgress", "Dressrosa") ~= 0 then
					if workspace.Map.Ice.Door.CanCollide then
						if not Backpack:FindFirstChild("Key") then
							if (CFrame.new(4852, 5, 718).Position - LocalPlayer.Character.HumanoidRootPart.Position).magnitude < 5 then
								ComF("DressrosaQuestProgress", "Detective")
								Equip("Key")
							else
								tp(CFrame.new(4852, 5, 718))
							end
						else
							Equip("Key")
							tp(workspace.Map.Ice.Door.CFrame)
						end
					else
						Kill({Monster = 'Ice Admiral',WaitMonster = false,Bring = false,Stop = function()
							return not C()['Second Sea']
						end})
					end
				else
					ComF("TravelDressrosa")
				end
			end
		end)
	end
end)

local FruitPrice, FruitStore = {}, {}
local whitelistedfruit = {}
local TableDevilFruit = {}
local m1 = {}
for i,v in next,ComF("GetFruits") do
	if v.Price >= 999999 then
		table.insert(whitelistedfruit,string.split(v.Name,"-")[1].." Fruit")
		m1[v.Name] = v.Price
	end
	table.insert(TableDevilFruit,v.Name)
end

function GetfruitstoreW3(x)
	local max = math.huge
	local name 
	for i,v in next,ComF("getInventory") do
		if v.Type == "Blox Fruit" then
			if not x then
				for i2, v2 in pairs(m1) do
					if v.Name == i2 then
						if tonumber(v2) < tonumber(max) then
							max = v2
							name = i2
						end
					end
				end
			else
				if not m1[v.Name] then
					return v.Name
				end
			end
		end
	end
	return name
end

function Checkdevilfruit()
	local name
	for i,v in pairs(Backpack:GetChildren()) do
		if string.find(v.Name,"Fruit")  then
			name = v.Name
		end
	end
	for i,v in pairs(Character:GetChildren()) do
		if string.find(v.Name,"Fruit")  then
			name = v.Name
		end
	end
	return name
end

Loop(function()
	if C()['Third Sea'] then
		pcall(function()
			local Level = Data.Level.Value
			if Data.Level.Value >= 1500 then
				if ComF("BartiloQuestProgress","Bartilo") == 3 then
					if ComF("GetUnlockables").FlamingoAccess ~= nil then							
						if ComF("ZQuestProgress","Check") == 0 then
							ComF("ZQuestProgress","Begin")
