VerifyMonster = function(v)
	if type(v) == 'string' then
		return (Enemies:FindFirstChild(v) and Enemies:FindFirstChild(v).Humanoid.Health >= 0) or (ReplicatedStorage:FindFirstChild(v) and ReplicatedStorage:FindFirstChild(v).Humanoid.Health >= 0)
	end
end


local Bring_ = 200
BringMob = function(targetMob)
	if not C()['Bring Monster'] then return end

	for _, player in ipairs(Players:GetPlayers()) do
		if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
			local distance = (player.Character.HumanoidRootPart.Position - HumanoidRootPart.Position).Magnitude
			if distance <= 500 then
				break
			else
				continue
			end
		end
	end

	local count = 0

	for _, mob in ipairs(Enemies:GetChildren()) do
		if count >= 2 then break end
		if mob:FindFirstChild("Humanoid") and mob:FindFirstChild("HumanoidRootPart") and not mob:GetAttribute("IsBoss") and mob.Humanoid.Health > 0 and (mob.HumanoidRootPart.Position - HumanoidRootPart.Position).Magnitude <= Bring_ then
			mob.HumanoidRootPart.CFrame = targetMob:GetPivot()
			mob.HumanoidRootPart.CanCollide = false
			mob.Humanoid.JumpPower = 0
			mob.Humanoid.WalkSpeed = 0

			if mob.Humanoid:FindFirstChild("Animator") then
				mob.Humanoid.Animator:Destroy()
			end

			pcall(function()
				sethiddenproperty(LocalPlayer, "SimulationRadius", math.huge)
			end)
			count += 1
		end
	end
end

Item = function(u)
	for _, container in ipairs({ComF("getInventoryWeapons"), Backpack, Character}) do
		for _, item in pairs(container:GetChildren()) do
			if item.Name == u then
				return true
			end
		end
	end
	return false
end

local MobBlacklist = {}

function getCleanName(name)
	return name:match("^(.-) ?%p?Lv%.? %d+%p?$") or name
end

function getMatchingParts(name)
	local cleanedName = getCleanName(name)
	local results = {}
	for _, s in pairs({EnemySpawns:GetChildren(), getnilinstances()}) do
		for _, v in ipairs(s) do
			if v:IsA("Part") and getCleanName(v.Name) == cleanedName then
				table.insert(results, v)
			end
		end
	end
	return results
end

WaitMonster = function(path, stop)
	pcall(function()
		local names = typeof(path) == "table" and path or {path}
		if #MobBlacklist >= 4 then MobBlacklist = {} return end
		for _, name in ipairs(names) do
			if not VerifyMonster(Name) and not table.find(MobBlacklist, name) then
				local parts = getMatchingParts(name)
				for _, part in ipairs(parts) do
					repeat
						tp(part.CFrame * CFrame.new(0, 40, 0))
						task.wait(0.5)
					until (part.Position - HumanoidRootPart.Position).Magnitude <= 50
						or (stop and stop()) or VerifyMonster(Name)
					break
				end
			end
		end
	end)
end

getClosestEnemy = function()
	local closest, closestDist = nil, math.huge
	for _, enemy in pairs(Enemies:GetChildren()) do
		if enemy:IsA("Model") and enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and Monster(enemy.Name) then
			local dist = (LocalPlayer.Character.HumanoidRootPart.Position - enemy.HumanoidRootPart.Position).Magnitude
			if dist < 2000 and dist < closestDist then
				closest, closestDist = enemy, dist
			end
		end
	end
	return closest
end

Monster = function(Mon, callback)
	local closestEnemy = nil
	local shortestDistance = math.huge
	for _, v in pairs(Enemies:GetChildren()) do
		if ((type(Mon) == "string" and v.Name == Mon) or (type(Mon) == "table" and table.find(Mon, v.Name))) 
			and v:FindFirstChild("Humanoid") 
			and v:FindFirstChild("HumanoidRootPart") 
			and v.Humanoid.Health > 0 then

			local distance = (v.HumanoidRootPart.Position - Character.HumanoidRootPart.Position).Magnitude
			if distance < shortestDistance then
				shortestDistance = distance
				closestEnemy = v
			end
		end
	end
	for _, v in pairs(ReplicatedStorage:GetChildren()) do
		if ((type(Mon) == "string" and v.Name == Mon) or (type(Mon) == "table" and table.find(Mon, v.Name))) 
			and v:FindFirstChild("Humanoid") 
			and v:FindFirstChild("HumanoidRootPart") 
			and v.Humanoid.Health > 0 then

			local distance = (v.HumanoidRootPart.Position - Character.HumanoidRootPart.Position).Magnitude
			if distance < shortestDistance then
				shortestDistance = distance
				closestEnemy = v
			end
		end
	end
	if closestEnemy and callback then
		callback(closestEnemy)
	end
	return closestEnemy
end

Kill = function(info)
	pcall(function()
		local Mon = info.Monster or 'Bandit'
		local Wait = info.WaitMonster or false
		local Bring = info.Bring
		local Callback = info.Callback or function() end
		local Stop = info.Stop or function() return end
		if Monster(Mon) then
			Monster(Mon, function(v)
				repeat
					tp(v:GetPivot() + Vector3.new(0, 40, 0))
					task.spawn(function()
						if Bring then
							BringMob(v)
						end 
					end)
					Callback(v)
					task.wait()
				until (Stop() or Stop)
			end)
		else
			if Wait then
				WaitMonster(Mon, function()
					return (Stop() or Stop)
				end)
			end
		end
	end)
end

Loop(function()
	if not C()['Level'] then return end
	if not Character then return end
	local Quest = Questc()
	local mon = string.gsub(string.gsub(string.gsub(PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "^Defeat %d+ ", ""), " %(.-%)$", ""), "s$", "")
	if not PlayerGui.Main.Quest.Visible then
		if (HumanoidRootPart.Position - Quest[4].Position).Magnitude <= 20 then
			task.wait(.5)
			ComF("StartQuest", Quest[2], Quest[3])
		else
			tp(Quest[4])
		end
	elseif not string.match(mon, Quest[1]) then
		ComF("AbandonQuest")
		wait(.5)
	elseif PlayerGui.Main.Quest.Visible and string.match(mon, Quest[1]) then
		Kill({Monster = Quest[1],WaitMonster = true,Bring = true,Stop = function()
			return not C()['Level'] or not PlayerGui.Main.Quest.Visible
		end})
	else
		ComF("AbandonQuest")
	end
end)

Loop(function()
	if not C()['Nearest'] then return end
	pcall(function()
		local closestEnemy = nil
		local closestDistance = math.huge
		for _, enemy in pairs(Enemies:GetChildren()) do
			if enemy:IsA("Model") and enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and Monster(enemy.Name) then
				local dist = (LocalPlayer.Character.HumanoidRootPart.Position - enemy.HumanoidRootPart.Position).Magnitude
				if dist < 2000 and dist < closestDistance then
					closestDistance = dist
					closestEnemy = enemy
				end
			end
		end
		if closestEnemy then
			Kill({Monster = closestEnemy.Name,WaitMonster = true,Bring = true,Stop = function()
				return not C()['Nearest']
			end})
		end
	end)
end)

Loop(function()
	if C()['Set Spawn'] then
		if Character.Humanoid.Health > 0 then
			ComF("SetSpawnPoint")
		end
	end
	if C()['V4'] then
		pcall(function()
			if Character:FindFirstChild("RaceEnergy") and Character.RaceEnergy.Value >= 1 and Character:FindFirstChild("RaceTransformed") and not Character.RaceTransformed.Value then
				Backpack.Awakening.RemoteFunction:InvokeServer(true)
			end
		end)
	end
	if C()['V3'] then
		if Character.Humanoid.Health > 0 then
			CommE:FireServer("ActivateAbility")
		end
	end
	pcall(function()
		if C()['Walk on Water'] then
			workspace.Map["WaterBase-Plane"].Size = Vector3.new(1000,112,1000)
		else
			workspace.Map["WaterBase-Plane"].Size = Vector3.new(1000,80,1000)
		end
	end)
	pcall(function()
		if C()['Store Fruit'] then
			for _, container in ipairs({Backpack, Character}) do
				for _, v in ipairs(container:GetChildren()) do
					if string.find(v.Name, "Fruit") then
						ComF("StoreFruit", tostring(v:GetAttribute("OriginalName")), v)
					end
				end
			end
		end
	end)
	task.wait(3)
end)

