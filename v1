	Allraid = {"Dark","Sand","Magma","Rumble","Flame","Ice","Light","Quake","Human: Buddha","Flame","Bird: Phoenix","Dough"}
	Default('Select Raid', Allraid[1])
	WeAreLoop(function()
		if C()['Buy Chip'] then
			ComF("RaidsNpc","Select",C()['Select Raid'])
			wait(3)
		end
	end)
	local Raid = Section(_Auto, translate("Raid", "ดันเจี้ยน"), 'l')
	Raid:CreateDropdown({Title = translate("Select Raid", "เลือกดัน"),List = Allraid,Value = C()['Select Raid'],Multi = false,Callback = function(v)
		C()['Select Raid'] = v
		Save('Select Raid', v)
	end})
	Raid:Line()
	Toggle(Raid, translate("Automatic Buy Chip", "ออโต้ซื้อชิป"),'Buy Chip')
	Toggle(Raid, translate("Automatic Start Raid", "ออโต้เริ่มดันเจี้ยน"),'Auto Start Raid')
