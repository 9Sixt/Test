	Materail_:SetTitle(translate("Select : ", "เลือก : ") .. html(tostring(C()['SelectMaterial']), Color3.fromRGB(0, 170, 255)))
	Materail_:SetImage(NumtoRbx(ImageMeterail))
end)
Default("SelectMaterial", AllMaterial[1])
