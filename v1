Loop(function()
	if C()['Auto Kill Player V4'] then
		local closestEnemy = nil
		local closestDistance = math.huge
		for _, v in pairs(Players:GetPlayers()) do
			if v.Character and IsAlive(v.Character) and v ~= LocalPlayer then
				local dist = (HumanoidRootPart.Position - v.Character:GetPivot().Position).Magnitude
				if dist < 1500 and dist < closestDistance then
					closestDistance = dist
					closestEnemy = v.Character
				end
			end
		end
		if closestEnemy then
			repeat task.wait()
				tp(closestEnemy:GetPivot() + Vector3.new(0, 40, 0))
			until not closestEnemy or not C()['Auto Kill Player V4'] or not IsAlive(closestEnemy)
		end
	end
end)

