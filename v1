Loop(function()
	if C()['Castle Raid'] then
		pcall(function()
			if (CFrame.new(-5118, 314, -2958).Position - HumanoidRootPart.Position).Magnitude <= 1000 then
				local closestEnemy = nil
				local closestDistance = math.huge
				for _, enemy in pairs(Enemies:GetChildren()) do
					if enemy:IsA("Model") and enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and Monster(enemy.Name) then
						local dist = (LocalPlayer.Character.HumanoidRootPart.Position - enemy.HumanoidRootPart.Position).Magnitude
						if dist < 2000 and dist < closestDistance then
							closestDistance = dist
							closestEnemy = enemy
						end
					end
				end
				if closestEnemy then
					Kill({Monster = closestEnemy.Name,WaitMonster = false,Bring = true,Stop = function()
						return not C()['Castle Raid']
					end})
				end
			elseif (CFrame.new(-5118, 314, -2958).Position - HumanoidRootPart.Position).Magnitude >= 3000 then
				Teleport(Vector3.new(-5106.0595703125, 314.8215026855469, -3180.6748046875))
			else
				tp(CFrame.new(-5118, 314, -2958))
			end
		end)
	end
end)

