	C()['SelectMaterial'] = v
	if v then
		if w1 then
			if (v == "Magma Ore") then
				ImageMeterail = ServiceIcon.mafmaore
			elseif v == "Leather" then
				ImageMeterail = ServiceIcon.lether
			elseif v == "Scrap Metal" then
				ImageMeterail = ServiceIcon.scrap
			elseif (v == "Angel Wings") then
				ImageMeterail = ServiceIcon.wing
			elseif (v == "Fish Tail") then
				ImageMeterail = ServiceIcon.fish
			elseif (v == "Yeti Fur") then
				ImageMeterail = ServiceIcon.yetifool
			elseif (v == "GunPowder") then
				ImageMeterail = ServiceIcon.gunpowder
			end
		end
		if w2 then
			if (v == "Magma Ore") then
				ImageMeterail = ServiceIcon.mafmaore
			elseif (v == "Scrap Metal") then
				ImageMeterail = ServiceIcon.scrap
			elseif (v == "Radioactive Material") then
				ImageMeterail = ServiceIcon.greenstone
			elseif (v == "Vampire Fang") then
				ImageMeterail = 119740006429586
			elseif (v == "Mystic Droplet") then
				ImageMeterail = ServiceIcon.mysticaldrop
			elseif (v == "Meteorite") then
				ImageMeterail = ServiceIcon.meteor
			end
		end
		if w3 then
			if (v == "Mini Tusk") then
				ImageMeterail = ServiceIcon.minitusk
			elseif (v == "Fish Tail") then
				ImageMeterail = ServiceIcon.fish
			elseif (v == "Scrap Metal") then
				ImageMeterail = ServiceIcon.scrap
			elseif (v == "Dragon Scale") then
				ImageMeterail = ServiceIcon.dragonscale
			elseif (v == "Conjured Cocoa") then
				ImageMeterail = 115826812289109
			elseif (v == "Demonic Wisp") then
				ImageMeterail = 92950011320841
			elseif (v == "Gunpowder") then
				ImageMeterail = ServiceIcon.gunpowder
			end
		end
	end
	Save('SelectMaterial', v)
end})
Materails:Line()
