		Nongkhaw:SetImage(111362591084511)
	end)
end

local General = Section(_General, translate("General", "ทั่วไป"), "r") do
	local PlayerInfo = {
		Level = General:CreateLabel({Title = translate("Level : ", "เลเวล : ") .. html(tostring(formatnumber(tonumber(Data.Level.Value))), Color3.fromRGB(255, 255, 0)),Side = 'Left'});
		Race = General:CreateLabel({Title = translate("Race : ", "เผ่า : ") .. html(Data.Race.Value, Color3.fromRGB(0, 170, 255)),Side = 'Left'});
		Beli = General:CreateLabel({Title = translate("Beli : ", "เงิน : ") .. html(tostring(formatnumber(tonumber(Money.Value))), Color3.fromRGB(0, 255, 127)),Side = 'Left'});
		Fragment = General:CreateLabel({Title = translate("Fragments : ", "แฟร็กเม้นต์ : ") .. html(tostring(formatnumber(tonumber(Fragments.Value))), Color3.fromRGB(107, 21, 255)),Side = 'Left'});
	}
	Loop(function()
		pcall(function()
			PlayerInfo.Level:Set(translate("Level : ", "เลเวล : ") .. html(tostring(formatnumber(tonumber(Data.Level.Value))), Color3.fromRGB(255, 255, 0)))
			PlayerInfo.Race:Set(translate("Race : ", "เผ่า : ") .. html(Data.Race.Value, Color3.fromRGB(0, 170, 255)))
			PlayerInfo.Beli:Set(translate("Beli : ", "เงิน : ") .. html(tostring(formatnumber(tonumber(Money.Value))), Color3.fromRGB(0, 255, 127)))
			PlayerInfo.Fragment:Set(translate("Fragments : ", "แฟร็กเม้นต์ : ") .. html(tostring(formatnumber(tonumber(Fragments.Value))), Color3.fromRGB(107, 21, 255)))
			task.wait(1)
		end)
	end)

	General:Line()
	Toggle(General, translate("Automatic Farm Level", "ออโต้ฟาร์มเลเวล"), "Level")
	if Env_.w1 then
		General:Line()
		Toggle(General, translate("Automatic Second Sea", "ออโต้ไปโลก 2"), "Second Sea")
		General:Line()
	end
	if Env_.w2 then
		General:Line()
		Toggle(General, translate("Automatic Third Sea", "ออโต้ไปโลก 3"), "Third Sea")
		General:Line()
	end
	if Env_.w3 then
		General:Line()
	end
	Toggle(General, translate("Automatic Farm Nearest", "ออโต้มอนรอบๆ"), "Nearest")
	General:Line()
	Button(General, translate("Redeem All Code", "ใส่โค้ดทั้งหมด"), AllCode)
end

