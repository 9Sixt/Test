local success, bosses = pcall(function()
	return loadstring(game:HttpGet("https://raw.githubusercontent.com/MerrySubs4t/Softwork/main/Fetching/DateBoss.lua"))()
end)

if success then
	for _, boss in ipairs(bosses) do
		WeAreLoop(function()
			if C()[boss.setting] then
				pcall(function()
					if not boss.world or _G[boss.world] then
						local m = Monster(boss.name)
						if m then
							KillBoss(m, function()
								return not C()[boss.setting]
							end)
						elseif boss.hop and C()[boss.hop] then
							HopServer()
						end
					end
				end)
			end
		end)
	end
else
	LocalPlayer:Kick("Cant Preloaded Data Boss ( . . . ) ")
end


