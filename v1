		for _, v in ipairs(Service.Players:GetDescendants()) do
			if v:IsA("Tool") and v.Name == "God's Chalice" then
				local player = v:FindFirstAncestorOfClass("Player")
				if player then
					local english = html(player.Name, Color3.fromRGB(0, 170, 255)) .. " has " .. html("God's Chalice", Color3.fromRGB(255, 255, 127))
					local thai = html(player.Name, Color3.fromRGB(0, 170, 255)) .. " มี " .. html("ถ้วยเสกแอดมิน", Color3.fromRGB(255, 255, 127))
					Indra_:SetDesc(translate(english, thai))
				end
			else
				Indra_:SetDesc(translate("Anyone dont have God's Chalice", "ไม่มีใครได้ถ้วยเสกแอดมิน"))
			end
