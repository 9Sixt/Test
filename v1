tp = function(Position)
	if not Character then return end
	local Distance = (Position.Position - HumanoidRootPart.Position).Magnitude
	if Distance > 0 and Distance < 300 then
		return TweenService:Create(HumanoidRootPart,TweenInfo.new(0.08, Enum.EasingStyle.Linear),{CFrame = Position}):Play()
	else
		local tw = TweenService:Create(HumanoidRootPart,TweenInfo.new(Distance / 300, Enum.EasingStyle.Linear),{CFrame = Position})
		function StopTween()
			if tw then
				tw:Cancel()
			end
		end
		return tw:Play()
	end
end

onetp = function(Position)
	repeat task.wait()
		tp(Position)
		if (Position.Position - HumanoidRootPart.Position).Magnitude < 10 then
			pcall(StopTween)
		end
	until (Position.Position - HumanoidRootPart.Position).Magnitude < 5
end

tpboat = function(model, goToCFrame)
	local seat = model:FindFirstChild("VehicleSeat")
	if seat and seat:IsA("BasePart") then
		local distance = (seat.Position - goToCFrame.Position).Magnitude
		local duration = distance / 300
		local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear)
		local tween = TweenService:Create(seat, tweenInfo, {CFrame = goToCFrame})
		tween:Play()
		function Untpboat()
			tween:Cancel()
		end
	end
end


RenderStepped:Connect(function()
	if not C()['Auto Farm Ken Level'] then
		if tablecheck(Env_.Configs, index) then
			pcall(Attack)
		end
	end
end)

Default('Weapon', "Melee")
RenderStepped:Connect(function()
	if tablecheck(Env_.Configs, index) then
		if not C()['Auto Sea Events Fully'] then
			pcall(function()
				for _, p in pairs(LocalPlayer.Backpack:GetChildren()) do
					if p:IsA("Tool") and p.ToolTip == C()['Weapon'] then
						if LocalPlayer.Backpack:FindFirstChild(p.Name) then
							LocalPlayer.Character.Humanoid:EquipTool(p)
						end
					end
				end
			end)
		end
		if not LocalPlayer.Character:FindFirstChild("HasBuso") then
			ComF("Buso")
		end
	end
end)

Loop(function()
	if tablecheck(Env_.Configs, index) then
		pcall(function()
			if not Character then return end
			local healthPercent = Humanoid.Health / Humanoid.MaxHealth
			if Character:WaitForChild("Humanoid").Sit then
				if not C()['Auto Sea Events Fully'] then
					Character:WaitForChild("Humanoid").Sit = false
				end
			end
			if not HumanoidRootPart:FindFirstChild("BodyVelocity1") then
				local BodyVelocity = Instance.new("BodyVelocity")
				BodyVelocity.Name = "BodyVelocity1"
				BodyVelocity.Parent = HumanoidRootPart
				BodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
				BodyVelocity.Velocity = Vector3.new(0, 0, 0)
			end
			if not Character:FindFirstChild("IsHighlight") then
				local HL = Instance.new("Highlight")
				HL.Name = 'IsHighlight'
				HL.Parent = Character
				HL.Adornee = Character
				HL.OutlineColor = Color3.fromRGB(255, 255, 255)
				if Humanoid then
					if healthPercent < 0.2 then
						HL.FillColor = Color3.fromRGB(255, 0, 0)
					elseif healthPercent < 0.5 then
						HL.FillColor = Color3.fromRGB(255, 255, 0)
					else
						HL.FillColor = Color3.fromRGB(0, 170, 255)
					end
				end
				HL.FillTransparency = 0.5
			else
				local HL = Character:FindFirstChild("IsHighlight")
				if HL and Humanoid then
					if healthPercent < 0.2 then
						HL.FillColor = Color3.fromRGB(255, 0, 0)
					elseif healthPercent < 0.5 then
						HL.FillColor = Color3.fromRGB(255, 255, 0)
					else
						HL.FillColor = Color3.fromRGB(0, 170, 255)
					end
				end
			end
			for _, v in pairs(Character:GetChildren()) do
				if v:IsA("BasePart") then
					v.CanCollide = false
				end
			end
		end)
	else
		pcall(StopTween)
		if HumanoidRootPart:FindFirstChild("BodyVelocity1") and Character:FindFirstChild("IsHighlight") then
			HumanoidRootPart:FindFirstChild("BodyVelocity1"):Destroy()
			Character:FindFirstChild("IsHighlight"):Destroy()
		end
	end
end)

Skill = function(s)
	for _, v in ipairs(s) do
		VirtualInputManager:SendKeyEvent(true, v, false, game)
		wait(.1)
		VirtualInputManager:SendKeyEvent(false, v, false, game)
	end
end


