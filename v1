		pcall(function()
			if LocalPlayer.Character:FindFirstChild("RaceEnergy") then
				if LocalPlayer.Character.RaceEnergy.Value >= 1 and LocalPlayer.Character:FindFirstChild("RaceTransformed") and not LocalPlayer.Character.RaceTransformed.Value then
					tp(CFrame.new(-9515.3720703125, 164.00624084473, 5786.0610351562))
					Backpack.Awakening.RemoteFunction:InvokeServer(true)
					wait()
					ComF("UpgradeRace", "Check")
					ComF("UpgradeRace", "Buy")
				else
					if Monster(MobT) then
						Kill(Monster(MobT), function()
							return not C()['Auto Train V4']
						end)
					else
						WaitMonster(MobT, function()
							return not C()['Auto Train V4']
						end)
					end
				end
			end
		end)
	end
end)

WeAreLoop(function()
	pcall(function()
		if C()['Rainbow Haki'] then
			if PlayerGui.Main.Quest.Visible == false then
				ComF("HornedMan","Bet")
			elseif PlayerGui.Main.Quest.Visible == true and string.find(PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Stone") then
				if Monster("Stone") then
					KillBoss(Monster("Stone"), function()
						return not C()['Rainbow Haki'] or not PlayerGui.Main.Quest.Visible
					end)
				else
					if C()['HOP Rainbow Haki'] then
						task.wait(3)
						HopServer()
					end
				end
			elseif PlayerGui.Main.Quest.Visible == true and string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Island Empress") then
				if Monster("Hydra Leader") then
					KillBoss(Monster("Hydra Leader"), function()
						return not C()['Rainbow Haki'] or not PlayerGui.Main.Quest.Visible
					end)
				else
					if C()['HOP Rainbow Haki'] then
						task.wait(3)
						HopServer()
					end
				end
			elseif string.find(PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Kilo Admiral") then
				if Monster("Kilo Admiral") then
					KillBoss(Monster("Kilo Admiral"), function()
						return not C()['Rainbow Haki'] or not PlayerGui.Main.Quest.Visible
					end)
				else
					if C()['HOP Rainbow Haki'] then
						task.wait(3)
						HopServer()
					end
				end
			elseif string.find(PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Captain Elephant") then
				if Monster("Captain Elephant") then
					KillBoss(Monster("Captain Elephant"), function()
						return not C()['Rainbow Haki'] or not PlayerGui.Main.Quest.Visible
					end)
				else
					if C()['HOP Rainbow Haki'] then
						task.wait(3)
						HopServer()
					end
				end
			elseif string.find(LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Beautiful Pirate") then
				if Monster("Beautiful Pirate") then
					KillBoss(Monster("Beautiful Pirate"), function()
						return not C()['Rainbow Haki'] or not PlayerGui.Main.Quest.Visible
					end)
				else
					if C()['HOP Rainbow Haki'] then
						task.wait(3)
						HopServer()
					end
				end
			else
				ComF("HornedMan","Bet")
			end
		end
	end)
end)

