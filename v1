local IslandSec = Section(_Teleport, translate("Island", "เกาะ"), 'l')
IslandList = {}
for _, v in pairs(workspace.Map:GetChildren()) do
	table.insert(IslandList, v.Name)
end
Default('Select Island', IslandList[1])
IslandSec:CreateDropdown({Title = translate("Select Island", "เลือกเกาะต่างๆ"),List = IslandList,Value = C()['Select Island'],Multi = false,Callback = function(v)
	C()['Select Island'] = v
	Save('Select Island', v)
end})
IslandSec:Line()
Toggle(IslandSec, translate("Teleport to Island", "เทเลพอร์ตไปที่เกาะ"), 'Teleport Island')


local PlayersSec = Section(_Teleport, translate("Players", "ผู้เล่น"), 'r')
PlayerList = {}
for _, v in pairs(Service.Players:GetChildren()) do
	table.insert(PlayerList, v.Name)
end
Default('Select Player', PlayerList[1])
local PlayerDropdown = PlayersSec:CreateDropdown({Title = translate("Select Island", "เลือกเกาะต่างๆ"),List = PlayerList,Value = C()['Select Player'],Multi = false,Callback = function(v)
	C()['Select Player'] = v
	Save('Select Player', v)
end})
Button(PlayersSec, translate("Refresh", "รีเฟรช"), function()
	PlayerDropdown:Clear()
	for _, v in pairs(Service.Players:GetChildren()) do
		PlayerDropdown:AddList(v.Name)
	end
end)
PlayersSec:Line()
Toggle(PlayersSec, translate("Teleport to Players", "เทเลพอร์ตไปหาผู้เล่น"), 'Teleport Player')


local PerformantSec = Section(_Misc, translate("Performant", "เพิ่มประสิทธิภาพ"), 'l')
Toggle(PerformantSec, translate("White Screen", "เปิดจอขาว"), 'White Screen', function(v)
	if v then
		game:GetService("RunService"):Set3dRenderingEnabled(false)
	else
		game:GetService("RunService"):Set3dRenderingEnabled(true)
	end
end)
Toggle(PerformantSec, translate("Automatic Rejoin", "ออโต้รีจอยน์"), 'Rejoin')
PerformantSec:Line()
Button(PerformantSec, translate("Low Quality Graphics", "โหมดกราฟฟิกต่ำ"), function()
	local Terrain = workspace:FindFirstChildOfClass('Terrain')
	Terrain.WaterWaveSize = 0
	Terrain.WaterWaveSpeed = 0
	Terrain.WaterReflectance = 0
	Terrain.WaterTransparency = 0
	Service.Lighting.GlobalShadows = false
	Service.Lighting.FogEnd = 9e9
	settings().Rendering.QualityLevel = 1
	for i,v in pairs(game:GetDescendants()) do
		if v:IsA("Part") or v:IsA("UnionOperation") or v:IsA("MeshPart") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
		elseif v:IsA("Decal") then
			v.Transparency = 1
		elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
			v.Lifetime = NumberRange.new(0)
		elseif v:IsA("Explosion") then
			v.BlastPressure = 1
			v.BlastRadius = 1
		end
	end
	for i,v in pairs(Service.Lighting:GetDescendants()) do
		if v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("BloomEffect") or v:IsA("DepthOfFieldEffect") then
			v.Enabled = false
		end
	end
	workspace.DescendantAdded:Connect(function(child)
		task.spawn(function()
			if child:IsA('ForceField') then
				heartbeat:Wait()
				child:Destroy()
			elseif child:IsA('Sparkles') then
				heartbeat:Wait()
				child:Destroy()
			elseif child:IsA('Smoke') or child:IsA('Fire') then
				heartbeat:Wait()
				child:Destroy()
			end
		end)
	end)
end)


local ServerSec = Section(_Misc, translate("Server", "เซิร์ฟเวอร์"), 'r')
Default('JobId', tostring(jobid))
ServerSec:CreateTextbox({
	Title = translate("JobId", "ใส่เลขไอดีเซิร์ฟ"),
	ClearTextOnFocus = true,
	Value = C()['JobId'],
	Callback = function(value)
		C()['JobId'] = value
		Save('JobId', value)
	end
})
ServerSec:Line()
Button(ServerSec, translate("Join", "เข้าร่วม"), function()

end)
ServerSec:Line()
Button(ServerSec, translate("Rejoin Server", "รีจอยน์เซิร์ฟเวอร์"), function()
	if #Service.Players:GetPlayers() <= 1 then
		LocalPlayer:Kick("\nRejoining...")
		wait()
		Service.TeleportService:Teleport(id, LocalPlayer)
	else
		Service.TeleportService:TeleportToPlaceInstance(id, jobid, LocalPlayer)
	end
end)
Button(ServerSec, translate("HOP Server", "ย้ายเซิร์ฟเวอร์"), function()
	pcall(HopServer)
end)


local MiscSec = Section(_Misc, translate("Others", "เพิ่มเติม"), 'l')
Button(MiscSec, translate("Join Pirate Team","เข้าร่วมฝั่งโจรสลัด"), function()
	ComF("SetTeam","Pirates")
	ComF("BartiloQuestProgress")
end)
Button(MiscSec, translate("Join Marine Team","เข้าร่วมฝั่งทหารเรือ"), function()
	ComF("SetTeam","Marines")
	ComF("BartiloQuestProgress")
end)
MiscSec:Line()
Button(MiscSec, translate("Enable Title List","เปิดหน้าต่างฉายา"), function()
	pcall(function()
		ComF("getTitles")
		PlayerGui.Main.Titles.Visible = true
	end)
end)
Button(MiscSec, translate("Enable Awakening Fruit List","เปิดหน้าต่างผลตื่น"), function()
	pcall(function()
		ComF("getAwakenedAbilities")
		PlayerGui.Main.AwakeningToggler.Visible = true
	end)
end)


local Configs = Section(_Misc, translate("Configs", "คอนฟิก"), 'r')
Button(Configs, translate("Load Configs","โหลดคอนฟิก"), function()
	pcall(Load)
end)
Configs:Line()
Button(Configs, translate("Save Configs","บันทึกคอนฟิก"), function()
	for key, value in pairs(_G.Configs) do
		Save(value, key)
	end
end)
Configs:Line()
Button(Configs, translate("Delete Configs","ลบคอนฟิก"), function()
	pcall(DeleteSettings)
end)


Service.GuiService.ErrorMessageChanged:Connect(function()
	if C()['Rejoin'] then
		if #Service.Players:GetPlayers() <= 1 then
			LocalPlayer:Kick("\nRejoining...")
			wait()
			Service.TeleportService:Teleport(id, LocalPlayer)
		else
			Service.TeleportService:TeleportToPlaceInstance(id, jobid, LocalPlayer)
		end
	end
end)
