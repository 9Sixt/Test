function Value(a)
	return translate("Price : ", "ราคา : ") .. html(tostring(a), Color3.fromRGB(0, 255, 127))
end

function PurpleValue(a)
	return translate("Fragment : ", "แฟ็กเม้นท์ : ") .. html(tostring(a), Color3.fromRGB(119, 21, 255))
end

