		elseif C()['UP_Healht'] then
			ComF("AddPoint", "Defense", 1)
		elseif C()['UP_Sword'] then
			ComF("AddPoint", "Sword", 1)
		elseif C()['UP_Gun'] then
			ComF("AddPoint", "Gun", 1)
		elseif C()['UP_Fruit'] then
			ComF("AddPoint", "Demon Fruit", 1)
		end
	end)
end)


local FirstCall = false
Loop(function()
	if C()['Auto Farm Ken Level'] then
		pcall(function()
			local observationData = {
				[1] = { name = "God's Guard", pos = CFrame.new(-4837, 850, -1840) },
				[2] = { name = "Zombie", pos = CFrame.new(-5497, 47, -795) },
				[3] = { name = "Mythological Pirate", pos = CFrame.new(-13234, 331, -7625) }
			}
			local worldIndex = Env_.w1 and 1 or Env_.w2 and 2 or Env_.w3 and 3
			if not worldIndex then return end
			local data = observationData[worldIndex]
			local Mon_Observaion = data.name
			local CFrame_Mon_Observation = data.pos
			if VerifyMonster(Mon_Observaion) then
				Monster(Mon_Observaion, function(v)
					repeat task.wait()
						local cur = tonumber(LocalPlayer:GetAttribute('KenDodgesLeft'))
						if cur == 0 then
							if C()['HOP Ken'] and not LocalPlayer:GetAttribute('KenActive') and not FirstCall then
								FirstCall = true
								pcall(Rejoin)
							else
								tp(v:GetPivot() + Vector3.new(0,40,0))
							end
						elseif cur == 1 or 2 then
							if not LocalPlayer:GetAttribute('KenActive')then
								VirtualUser:SetKeyDown('0x65')
								VirtualUser:SetKeyUp('0x65')
							end
							task.wait(0.5)
							tp(v:GetPivot() + Vector3.new(3,0,0))
						end
					until not C()['Auto Farm Ken Level'] or not IsAlive(v)
				end)
			else
				tp(CFrame_Mon_Observation)
			end
		end)
	end
end)

Loop(function()
	if C()['Raid Clear']  then
		pcall(function()
			if PlayerGui.Main.TopHUDList.RaidTimer.Visible and (Env_.w2 or Env_.w3) and LocalPlayer:GetAttribute("IslandRaiding") then
				local enemy = getClosestEnemy()
				if enemy then
					Kill({
						Monster = enemy.Name,
						WaitMonster = false,
						Bring = true,
						Stop = function() return not C()['Raid Clear'] end
					})
					BringMob(enemy)
				else
					for i = 5, 1, -1 do
						local island = Locations:FindFirstChild("Island " .. i)
						if island then
							tp(island.CFrame * CFrame.new(4, 80, 10))
							break
						end
					end
				end
			else
				if LocalPlayer.Backpack:FindFirstChild("Special Microchip") or Character:FindFirstChild("Special Microchip") then
					if Env_.w2 then
						fireclickdetector(workspace.Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)
					elseif Env_.w3 then
						fireclickdetector(workspace.Map["Boat Castle"].RaidSummon2.Button.Main.ClickDetector)
					end
				elseif not Blacklist_Executor then
					ComF("RaidsNpc", "Select", C()['Select Raid'])
				end
			end
		end)

	end
end)

local zones = { 
	[1] = CFrame.new(-21227, 175, 4047),
	[2] = CFrame.new(-24237, 175, 6381),
	[3] = CFrame.new(-27105, 175, 8959),
	[4] = CFrame.new(-29350, 175, 11744),
	[5] = CFrame.new(-32404, 175, 16208),
	[6] = CFrame.new(-35611, 175, 20548),
	[7] = CFrame.new(-999999, 175, 999999)
}

local zoneMap = {
	["Level 1 - Low"] = 1,
	["Level 2 - Medium"] = 2,
	["Level 3 - High"] = 3,
	["Level 3 - Extreme"] = 4,
	["Level 5 - Crazy"] = 5,
	["Level 6 - ???"] = 6
}

Loop(function()
	if not C()['Auto Sea Events Fully'] then
		pcall(Untpboat)
		return
	end
	if workspace.Boats:FindFirstChild('PirateGrandBrigade') then
		pcall(function()
			local closestEnemy = nil
			for _, v in pairs(Enemies:GetChildren()) do
				if table.find(C()['Select Seamob'], v.Name) and IsAlive(v) then
					local distance = (v:GetPivot().Position - HumanoidRootPart.Position).Magnitude
					if distance < 1500 then
						closestEnemy = v
					end
				end
			end
			if closestEnemy then
				pcall(Untpboat)
				Humanoid.Sit = false
				repeat task.wait()
					tp(closestEnemy:GetPivot() + Vector3.new(0, (Humanoid.Health <= Humanoid.MaxHealth * 0.3) and 300 or 50, 0))
					for _, p in pairs(LocalPlayer.Backpack:GetChildren()) do
						if p:IsA("Tool") and p.ToolTip == C()['Weapon'] then
							if LocalPlayer.Backpack:FindFirstChild(p.Name) then
								LocalPlayer.Character.Humanoid:EquipTool(p)
							end
						end
					end
				until not C()['Auto Sea Events'] or not IsAlive(closestEnemy)
				return
			end
			local boat = workspace.Boats['PirateGrandBrigade']
			local seat = boat:FindFirstChild("VehicleSeat")
			if not Humanoid.Sit then
				pcall(Untpboat)
				if seat then
					if (seat.Position - HumanoidRootPart.Position).Magnitude < 50 then
						tp(seat.CFrame + Vector3.new(0, 3, 0))
					else
						tp(boat:GetPivot())
					end

				else
					tp(boat:GetPivot())
				end
			else
				local selectedName = C()['Select Zone']
				local zoneIndex = zoneMap[selectedName] or 7
				tpboat(boat, zones[zoneIndex])
				pcall(function()
					if seat then
						if math.abs(seat.Position.Y - 175) > 1 then
							seat.CFrame = CFrame.new(seat.Position.X, 175, seat.Position.Z)
						end
						local bp = seat:FindFirstChild("BodyPosition")
						if bp then bp.MaxForce = Vector3.new(0, 0, 0) end
						local bv = seat:FindFirstChild("BodyVelocity")
						if bv then
							bv.MaxForce = Vector3.new(9e9, 9e9, 9e9)
							bv.P = 0
						end
					end
				end)
			end
		end)
	else
		if (CFrame.new(-16928, 9, 437).Position - HumanoidRootPart.Position).Magnitude <= 5 then
			pcall(function()
				ComF("BuyBoat", "PirateGrandBrigade")
			end)
			task.wait(0.5)
		else
			tp(CFrame.new(-16928, 9, 437))
		end
	end
end)
Loop(function()
	if C()['Drive Boat'] then
		if Humanoid.Sit then
			VirtualInputManager:SendKeyEvent(true, "W", false, game)
		end
	end
	if C()['Noclip Rock'] then
		pcall(function()
			if LocalPlayer.Character.Humanoid.Sit then
				for _, v in pairs(workspace.Boats:GetDescendants()) do
					if v:IsA("BasePart") and v.CanCollide == true then
						v.CanCollide = false
					end
				end
				for _, v in pairs(LocalPlayer.Character:GetDescendants()) do
					if v:IsA("BasePart") and v.CanCollide == true then
						v.CanCollide = false
					end
				end
			elseif not LocalPlayer.Character.Humanoid.Sit and not C()['Auto Sea Events Fully'] then
				for _, v in pairs(workspace.Boats:GetDescendants()) do
					if v:IsA("BasePart") and v.CanCollide == false then
						v.CanCollide = true
					end
				end
				for _, v in pairs(LocalPlayer.Character:GetDescendants()) do
					if v:IsA("BasePart") and v.CanCollide == false then
						v.CanCollide = true
					end
				end
			end
		end)
	end
end)

Loop(function()
	if C()['Auto Race V4'] then
		pcall(function()
			if LocalPlayer.Data.Race.Value == "Mink" then	
				if workspace.Map:FindFirstChild("MinkTrial") then
					if (workspace.Map.MinkTrial.Ceiling.CFrame - HumanoidRootPart.Position).Magnitude <= 200 then
						tp(workspace.Map.MinkTrial.Ceiling.CFrame * CFrame.new(0,-20,0))
					end
				end
			elseif LocalPlayer.Data.Race.Value == "Human" then
				if workspace.Map:FindFirstChild("HumanTrial") then
					for i,v in pairs(Enemies:GetChildren()) do
						if (v.HumanoidRootPart.Position - HumanoidRootPart.Position).magnitude <= 400 then
							repeat task.wait()
								tp(v:GetPivot() + Vector3.new(0, 40, 0))
							until not C()['Auto Race V4'] or not IsAlive(v)
						end
					end
				end
			elseif LocalPlayer.Data.Race.Value == "Cyborg" then
				if workspace.Map:FindFirstChild("CybrogTrial") then
					tp(workspace.Map.CyborgTrial.Floor.CFrame*CFrame.new(0,500,0))
				end
			elseif LocalPlayer.Data.Race.Value == "Skypiea" then
				if workspace.Map:FindFirstChild("SkyTrial") then
					if (workspace.Map.SkyTrial.Model.FinishPart.CFrame - HumanoidRootPart.Position).Magnitude <= 750 then
						tp(workspace.Map.SkyTrial.Model.FinishPart.CFrame)
					end
				end
			elseif LocalPlayer.Data.Race.Value == "Ghoul" then
				if workspace.Map:FindFirstChild("GhoulTrial") then 
					for i,v in pairs(workspace.Enemies:GetChildren()) do
						if (v.HumanoidRootPart.Position - HumanoidRootPart.Position).magnitude <= 400 then
							repeat task.wait()
								tp(v:GetPivot() + Vector3.new(0, 40, 0))
							until not C()['Auto Race V4'] or not IsAlive(v)
						end
					end
				end
			elseif LocalPlayer.Data.Race.Value == "Fishman" then
				if workspace.Map:FindFirstChild("FishTrial") then
					if (workspace.Map:FindFirstChild("FishTrial").Part.Position - HumanoidRootPart.Position).Magnitude <= 500 then
						tp(CFrame.new(25032.043, 75.04647064, 12619.5967))
						Skill({"Z", "X", "C", "V"})
						task.wait(.5)
					end
				end
			end
		end)
	end
end)


