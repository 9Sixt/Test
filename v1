	Toggle(Raid, translate("Kill Aura", "ออร่าแห่งความตาย"),'Kill Aura Raid')
	Toggle(Raid, translate("Automatic Buy Awakening", "ออโต้ซื้อสกิล"),'Auto Buy Awaken')
	Raid:Line()
	Toggle(Raid, translate("Automatic Unstore Fruit", "ออโต้เอาผลออกจากกล่อง"),'Auto Unstored Fruit')
	Raid:Line()
	Toggle(Raid, translate("Teleport to Lab", "เทเลพอร์ตไปที่ลงดัน"),'Teleport to Lab')
	WeAreLoop(function()
		if C()['Teleport to Lab'] then
			tp(CFrame.new(-6462, 250, -4518))
		end
	end)

end

