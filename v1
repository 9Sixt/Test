local BossAndMonster = Section(_General, translate("Boss", "บอส"), "r") do
	local CheckList = {}
	local tableboss = {}
	for _, v in pairs(Enemies:GetChildren()) do
		if IsAlive(v) and v:GetAttribute("IsBoss") then
			if not CheckList[v.Name] then
				table.insert(tableboss, v.Name)
				CheckList[v.Name] = true
			end
		end
	end
	for _, v in pairs(ReplicatedStorage:GetChildren()) do
		if IsAlive(v) and v:GetAttribute("IsBoss") then
			if not CheckList[v.Name] then
				table.insert(tableboss, v.Name)
				CheckList[v.Name] = true
			end
		end
	end
	local DropdownBoss = BossAndMonster:CreateDropdown({Title = translate("Select Boss", "เลือกบอส"),List = tableboss,Value = SelectBoss,Multi = false,Callback = function(value)
		SelectBoss = value
	end})
	Button(BossAndMonster, translate("Refresh", "รีเฟรช"), function()
		DropdownBoss:Clear()
		CheckList = {}
		for _, v in pairs(Enemies:GetChildren()) do
			if IsAlive(v) and v:GetAttribute("IsBoss") then
				if not CheckList[v.Name] then
					DropdownBoss:AddList(v.name)
					CheckList[v.Name] = true
				end
			end
		end
		for _, v in pairs(ReplicatedStorage:GetChildren()) do
			if IsAlive(v) and v:GetAttribute("IsBoss") then
				if not CheckList[v.Name] then
					DropdownBoss:AddList(v.name)
					CheckList[v.Name] = true
				end
			end
		end
	end)
	Toggle(BossAndMonster, translate("Kill Select Boss", "ฆ่าบอสที่เลือก"), "Kill Select Boss")
	Toggle(BossAndMonster, translate("Kill All Boss", "ฆ่าบอสทั้งหมด"), "Kill All Boss")
end

local Fruits = Section(_General, translate("Fruits", "ผลไม้"), "l") do
	local IsFruits = Fruits:CreateLabel({Title = translate("Fruits : ", "ผลปีศาจ : ") .. IsNotSpawn})
	Loop(function()
		pcall(function()
			for _,v in pairs(workspace:GetChildren()) do
				if string.find(v.Name, "Fruit") then
					IsFruits:Set(translate("Fruits : ", "ผลปีศาจ : ") .. IsSpawn)
					if C()['Fruit Find'] then
						tp(v.Handle.CFrame + Vector3.new(0, 2, 0))
					end
				else
					IsFruits:Set(translate("Fruits : ", "ผลปีศาจ : ") .. IsNotSpawn)
				end
			end
			if C()['Fruit Random'] then
				ComF("Cousin", "Buy")
			end
		end)
	end)
	Toggle(Fruits, translate("Automatic Find Fruits", "ออโต้หาผลปีศาจ"), "Fruit Find")
	Toggle(Fruits, translate("Automatic Random Fruits", "ออโต้สุ่มผลปีศาจ"), "Fruit Random")
	Toggle(Fruits, translate("Automatic Stored Fruits", "ออโต้เก็บผลปีศาจเข้ากระเป๋า"), "Store Fruit")
	Button(Fruits, translate("Random Fruits", "สุ่มผลปีศาจ"), function() ComF("Cousin", "Buy") end)
	Fruits:Line()
	Toggle(Fruits, translate("Automatic Find Berry", "ออโต้หาเบอร์รี่"), "Berry Find")
	if Env_.w3 then
		Fruits:Line()
		Toggle(Fruits, translate("Automatic Find Banana", "ออโต้หากล้วย"), "Banana Find")
		Toggle(Fruits, translate("Automatic Find Apple", "ออโต้หาแอปเปิ้ล"), "Apple Find")
		Toggle(Fruits, translate("Automatic Find Pineapple", "ออโต้หาสัปปะรด"), "Pineapple Find")
	end
end

