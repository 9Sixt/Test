		end
	end)


	function CheckAcientOneStatus()
		if not LocalPlayer.Character:FindFirstChild("RaceTransformed") then
			return ". . ."
		end
		local fragment, trained, status = nil, nil, nil
		status, trained, fragment = ComF("UpgradeRace", "Check")
		if status == 1 or status == 3 then
			return translate("You need to train more", "คุณต้องฝึกอีก")
		elseif status == 2 or status == 4 or status == 7 then
			return translate("You can buy gear with " .. html(tostring(fragment) .. " Fragment", Color3.fromRGB(149, 20, 255)))
		elseif status == 5 then
			return translate("You are done your race", "คุณได้อัพเผ่าของคุณแล้ว")
		elseif status == 6 then
			return translate("Upgrade completed: " .. (trained - 2) .. "/3, Need Trains More", "อัปเกรดแล้ว: " .. (trained - 2) .. "/3 ขั้น ต้องฝึกเพิ่มอีก")
		elseif status == 0 then
			return translate("Ready for next trials", "คุณสามารถเข้ารวมบททดสอบต่อไปได้")
		elseif status == 8 then
			return translate("Remaining " .. (10 - trained) .. " Training Sessions.", "เหลือการฝึกอีก " .. (10 - trained) .. " ครั้ง")
		else
			return ". . ."
		end
	end

	local Race = Section(_Auto, translate("Race", "เผ่า"), 'r')
	Race_ = Race:CreateImage({
		Title = translate("Race : ", "เผ่า : ") .. html(tostring(LocalPlayer.Data.Race.Value), Color3.fromRGB(0, 170, 255)),
		Desc = translate("Status : ", "สถานะ : ") .. Isnil,
		Icon = "http://www.roblox.com/asset/?id=107550685864886"
	})
	Toggle(Race, translate('Automatic Complete Trial', "ออโต้ชนะบททดสอบ"), 'Auto Trial V4')
	Toggle(Race, translate('Teleport to Trial Door', "เทเลพอร์ตไปประตูทำเผ่า"), 'Teleport to Door')
	Race:Line()
	Button(Race, translate("Teleport to Temple of Time", "เทเลพอร์ตไปที่วัดแห่งการเวลา"), function()
		ComF("requestEntrance", Vector3.new(28282.5703125, 14896.8505859375, 105.1042709350586))
	end)
	WeAreLoop(function()
		Race_:SetDesc(translate("Status : ", "สถานะ : ") .. CheckAcientOneStatus())
		task.wait(5)
	end)


	local BoneSection = Section(_Auto, translate("Haunted Castle", "ประสาทผีสิง"), 'l')
	Bone = BoneSection:CreateImage({Title = translate("Bones", "กระดูก"),Desc = translate("Value : ", "จำนวน : ") .. Isnil,Icon = "http://www.roblox.com/asset/?id=107550685864886"})
	WeAreLoop(function()
		pcall(function()
			Bone:SetDesc(translate("Value : ", "จำนวน : ") .. html(tostring(ComF("Bones","Check")), Color3.fromRGB(255, 170, 0)))
		end)
	end)
	BoneSection:Line()
	Toggle(BoneSection, translate("Automatic Farm Bones", "ออโต้ฟาร์มกระดูก"), "Farm Bone")
	BoneSection:Line()
	Toggle(BoneSection, translate("Random Bones", "สุ่มกระดูก"), "Random Bone")


	local CakeSection = Section(_Auto, translate("Sea of Treats", "ทะเลแห่งขนมหวาน"), 'r')
	Cake = CakeSection:CreateImage({
		Title = translate("Cake Prince : ", "คาตาคุริ : ") .. Isnil,
		Desc = translate("Required : ", "ต้องฆ่าอีกจำนวน : ") .. Isnil,
		Icon = "http://www.roblox.com/asset/?id=80630925030885"
	})
	CakeSection:Line()
	Toggle(CakeSection, translate("Automatic Cake Prince", "ออโต้ฟาร์มคาตาคุริ"), "Cake Prince")
	CakeSection:Line()
	Toggle(CakeSection, translate("Spawn Cake Prince", "ออโต้เสกคาตาคุริ"), 'Spawn Katakuri')
	WeAreLoop(function()
		pcall(function()
			if tonumber(string.match(tostring(ComF("CakePrinceSpawner")), "%d+")) ~= nil then
				local killsLeft = string.match(tostring(ComF("CakePrinceSpawner")), "%d+")
				local descText = translate("Required : ", "ต้องฆ่าอีกจำนวน : ") .. html(killsLeft, Color3.fromRGB(255, 116, 223)) .. translate(" Enemies", " ตัว")
				Cake:SetDesc(descText)
				Cake:SetTitle(translate("Cake Prince : ", "คาตาคุริ : ") .. IsNotSpawn)
			else
				Cake:SetDesc(translate(html("Cake Prince is Spawned", Color3.fromRGB(0, 255, 127)), html("คาตาคุริเกิดแล้ว", Color3.fromRGB(0, 255, 127))))
				Cake:SetTitle(translate("Cake Prince : ", "คาตาคุริ : ") .. IsSpawn)
				if C()['Spawn Katakuri'] then
					ComF("CakePrinceSpawner", true)
				end
			end
		end)
	end)


	local EliteSection = Section(_Auto, translate("Elite Hunters", "อีลีทฮันเตอร์"), 'l')
	Elite = EliteSection:CreateImage({
		Title = translate("Elite Hunters : ", "อีลีทฮันเตอร์ : ") .. Isnil,
		Desc = translate("Progress : ", "ฆ่าไปแล้ว : ") .. Isnil,
		Icon = "http://www.roblox.com/asset/?id=139078897425682"
	})
	Toggle(EliteSection, translate("Automatic Elite", "ออโต้ล่าอีลีท"), "Elite")
	Toggle(EliteSection, translate("HOP Elite", "ย้ายเซิฟหาอีลีท"), 'HOP Elite')
	local Elitemob = {"Diablo", "Deandre", "Urban"}
	WeAreLoop(function()
		pcall(function()
			Elite:SetDesc(translate("Progress : ", "ฆ่าไปแล้ว : ") .. html(tostring(ComF("EliteHunter","Progress")), Color3.fromRGB(0, 170, 255)) .. translate(" Elite", " ตัว"))
			if Monster(Elitemob) then
				Elite:SetTitle(translate("Elite Hunters : ", "อีลีทฮันเตอร์ : ") ..  IsSpawn)
			else
				Elite:SetTitle(translate("Elite Hunters : ", "อีลีทฮันเตอร์ : ") .. IsNotSpawn)
			end
		end)
	end)


