if Env_.w2 or Env_.w3 then
	local Raid = Section(_Auto, translate("Raid", "ดันเจี้ยน"), "l") do
		local FruitPricez, FruitStorez = {}, {}
		local whitelistedfruitz = {}
		local TableDevilFruitz = {}
		local m1z = {}
		for i,v in next,ComF("GetFruits") do
			if v.Price <= 1000000 then
				table.insert(whitelistedfruitz,string.split(v.Name,"-")[1].." Fruit")
				m1z[v.Name] = v.Price
			end
			table.insert(TableDevilFruitz,v.Name)
		end

		function Getfruitstore(x)
			local max = math.huge
			local name 
			for i,v in next,ComF("getInventory") do
				if v.Type == "Blox Fruit" then
					if not x then
						for i2, v2 in pairs(m1z) do
							if v.Name == i2 then
								if tonumber(v2) < tonumber(max) then
									max = v2
									name = i2
								end
							end
						end
					else
						if not m1z[v.Name] then
							return v.Name
						end
					end
				end
			end
			return name
		end

		Allraid = {"Dark","Sand","Magma","Rumble","Flame","Ice","Light","Quake","Human: Buddha","Flame","Bird: Phoenix","Dough"}
		Default('Select Raid', Allraid[1])
		Raid:CreateDropdown({Title = translate("Select Raid", "เลือกดัน"),List = Allraid,Value = C()['Select Raid'],Multi = false,Callback = function(v)
			C()['Select Raid'] = v
			Save('Select Raid', v)
		end})
		Raid:Line()
		Toggle(Raid, translate("Automatic Raid [ Attack Monster ]", "ออโต้ดันเจี้ยน"),'Raid Clear')
		Raid:Line()
		Toggle(Raid, translate("Automatic Buy Awakening", "ออโต้ซื้อสกิล"),'Auto Buy Awaken')
		Toggle(Raid, translate("Automatic Unstore Fruit", "ออโต้เอาผลออกจากกล่อง"),'Auto Unstored Fruit')
		Toggle(Raid, translate("Teleport to Lab", "เทเลพอร์ตไปที่ลงดัน"),'Teleport to Lab')
		Loop(function()
			if C()['Teleport to Lab'] then
				if Env_.w2 then
					tp(CFrame.new(-6462, 250, -4518))
				else
					tp(CFrame.new(-5016, 315, -2828))
				end
			end
			if C()['Auto Unstored Fruit'] then
				if not Checkdevilfruit() and Getfruitstore() then
					ComF("LoadFruit", Getfruitstore())
				end
			end
			if C()['Auto Buy Awaken'] then
				ComF("Awakener","Check")
				ComF("Awakener","Awaken")
			end
			task.wait(3)
		end)
	end
	--GuiService:ClearError()
	local SeaEvents = Section(_Auto, translate("Sea Events", "อีเว้นท์ทะเล"), "r") do
		Default('Select Zone', 'Level 5 - Crazy')
		Default('Select Seamob', {"Piranha", "Terrorshark","Shark","Fish Crew Member"})
		SeaEvents:CreateDropdown({Title = translate("Select Zone", "เลือกโซนทะเล"),List = {"Level 1 - Low","Level 2 - Medium","Level 3 - High","Level 4 - Extreme","Level 5 - Crazy","Level 6 - ???", "Level - Infinite"},Value = C()['Select Zone'],Multi = false,Callback = function(v)
			C()['Select Zone'] = v
			Save('Select Zone', v)
		end})
		SeaEvents:CreateDropdown({Title = translate("Select Monster", "เลือกมอนสเตอร์"),List = {"Shark","Terrorshark","Piranha","Fish Crew Member"},Value = C()['Select Seamob'],Multi = true,Callback = function(v)
			C()['Select Seamob'] = v
			Save('Select Seamob', v)
		end})
		Toggle(SeaEvents, translate("Automatic Hunt Sea Events", "ออโต้ล่าอีเว้นท์ทะเลตัวเต็ม"),'Auto Sea Events Fully')
		Toggle(SeaEvents, translate("Automatic Sea Events", "ออโต้ตีอีเว้นท์"),'Auto Sea Events')
		SeaEvents:Line()
		Toggle(SeaEvents, translate("Automatic Drive Boat [ W ]", "ออโต้ขับเรือ [ กดเดิน ]"),'Drive Boat')
		Toggle(SeaEvents, translate("Remove Rock", "เรือทะลุหี"),'Noclip Rock')
		local isBoosted = false
		Button(SeaEvents, translate("Boost Speed Boat"), function()
			local vehicleSeats = {}
			for i, v in pairs(workspace.Boats:GetDescendants()) do
				if v:IsA("VehicleSeat") then
					table.insert(vehicleSeats, v)
				end
			end
			isBoosted = not isBoosted
			for _, v in pairs(vehicleSeats) do
				v.MaxSpeed = isBoosted and 350 or 125
			end
		end)
	end
end

local Ken = Section(_Auto, translate("Observation", "ฮาคิสังเกตุ"), "l") do
	Toggle(Ken, translate('Automatic Observation EXP', "ออโต้ฟาร์มฮาคิสังเกตุ"), 'Auto Farm Ken Level')
	Toggle(Ken, translate('Rejoin When Ken Dodge : 0', "รีจอยน์ตอนการหลบหมด"), 'HOP Ken')
end


return warn("Authorization Success : " .. string.format("%.5f", tostring(Tick) ) .. " to Load Data " .. "User : " .. tostring(Name) .. " unpack(" .. _Folder .. ")")
