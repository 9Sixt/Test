tp = function(Position)
	local hrp = LocalPlayer.Character.HumanoidRootPart
	local Distance = (Position.Position - hrp.Position).Magnitude
	local CanTeleport = false

	if not LocalPlayer.Character:FindFirstChild("PartTele") then
		local partTele = Instance.new("Part", LocalPlayer.Character)
		partTele.Size = Vector3.new(10,1,10)
		partTele.Name = "PartTele"
		partTele.Anchored = true
		partTele.Transparency = 1
		partTele.CanCollide = false
		partTele.CFrame = hrp.CFrame
		partTele:GetPropertyChangedSignal("CFrame"):Connect(function()
			task.wait(0.01)
			hrp.CFrame = partTele.CFrame
		end)
	end

	local partTele = LocalPlayer.Character:FindFirstChild("PartTele")

	if Distance > 0 and Distance < 300 then
		local tw1 = Service.TweenService:Create(
			partTele,
			TweenInfo.new(0, Enum.EasingStyle.Linear),
			{CFrame = typeof(Position) == "CFrame" and Position or Position.CFrame}
		)
		tw1:Play()
		function StopTween()
			if tw1 then
				tw1:Cancel()
			end
			if LocalPlayer.Character:FindFirstChild("PartTele") then
				LocalPlayer.Character:FindFirstChild("PartTele"):Destroy()
			end
		end
		return task.wait(0.075)
	else
		if Distance > 300 then
			local closestDistance = math.huge
			local closestV = nil
			for i, v in ipairs(Canteleport) do
				if (v - Position.Position).Magnitude < (LocalPlayer.Character.HumanoidRootPart.Position - Position.Position).Magnitude
					and LocalPlayer.Character.Humanoid.Health > 0
					and (v - LocalPlayer.Character.HumanoidRootPart.Position).Magnitude > 500 then

					local distance = (v - Position.Position).Magnitude
					if distance < closestDistance then
						closestDistance = distance
						closestV = v
					end
				end
			end
			if C()['Upper Tween'] then
				if partTele.Position.Y ~= 200 then
					partTele.CFrame = CFrame.new(hrp.Position.X, 200, hrp.Position.Z)
				end
			end
			local tw = Service.TweenService:Create(
				partTele,
				TweenInfo.new(Distance / 300, Enum.EasingStyle.Linear),
				{CFrame = CFrame.new(Position.X, 200, Position.Z)}
			)
			function StopTween()
				if tw then
					tw:Cancel()
				end
				if LocalPlayer.Character:FindFirstChild("PartTele") then
					LocalPlayer.Character:FindFirstChild("PartTele"):Destroy()
				end
			end
			if closestV then
				if w3 and Monster({'Cake Prince', 'Dough King'}) and C()['Cake Prince']
					and (Vector3.new(-1990, 4532, -14973) - Position.Position).Magnitude < 1500 then
					if tw then
						tw:Cancel()
					end
					if workspace.Map.CakeLoaf.BigMirror:FindFirstChild("Main") then
						firetouchinterest(workspace.Map.CakeLoaf.BigMirror.Main, LocalPlayer.Character.HumanoidRootPart, 0)
						firetouchinterest(workspace.Map.CakeLoaf.BigMirror.Main, LocalPlayer.Character.HumanoidRootPart, 1)
					else
						LocalPlayer.Character.Character.HumanoidRootPart.CFrame = closestV
					end
					LocalPlayer.Character.PartTele.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame
					return
				end

				ComF("requestEntrance", closestV)
				task.wait(.5)
				LocalPlayer.Character.PartTele.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame
			else
				tw:Play()
			end
		end
	end
end

